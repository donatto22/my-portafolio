{
    "env": {
        "browser": true,
        "commonjs": true,
        "es2021": true
    },
    "extends": "standard",
    "parserOptions": {
        "ecmaVersion": "latest"
    },
    "rules": {
        "indent": "off",
        "no-unused-vars": "off",
        "no-trailing-spaces": "off",
        "array-callback-return": "off",
        "no-unreachable-loop": "off",
        "no-tabs": "off"
    }
}
